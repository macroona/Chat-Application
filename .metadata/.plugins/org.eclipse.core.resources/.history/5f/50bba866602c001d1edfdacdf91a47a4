import java.net.*;
public class Server {
	ServerSocket server;
	Socket client;
	
	Server()
	{
		try {
			server = new ServerSocket(7777);
		} catch (Exception e) {
			// TODO: handle exception
			e.printStackTrace();
		}
	}
	
	
	public static void main(String[] args)
	{
		System.out.println("This is server...going to start Server");
		// will call constructor
		new Server();
	}

}
